<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RippleTransitionAnimationView">
        <attr name="animator_ripple_color" format="color"/>
        <attr name="animator_ripple_radius" format="dimension"/>
        <attr name="animator_ripple_duration" format="enum">
            <enum name="animator_short" value="0"/>
            <enum name="animator_medium" value="1"/>
            <enum name="animator_long" value="2"/>
        </attr>
    </declare-styleable>
</resources>